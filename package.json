{
  "name": "twitter-clone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "nodemon",
    "migrate-start": "npm run migrate && npm start",
    "migrate-start-dev": "npm run migrate && npm run start-dev",
    "migrate": "node -r ./dotenv-config.js ./node_modules/knex/bin/cli.js migrate:latest --env databaseConfig",
    "makemigration": "./node_modules/knex/bin/cli.js migrate:make --env databaseConfig",
    "test": "mocha"
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "http-errors": "^1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "objection": "^1.6.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.9.0",
    "validator": "^10.11.0"
  }
}
